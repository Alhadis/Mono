.\" Identify the flavour of troff(1) processing the document
.ds $0 unknown
.ft R
.ie \n(.f=0 .ds $0 mandoc
.el \{\
.	ft
.	ie 0<>1 .ds $0 heirloom
.	el .ie \w,\X'# Ignore',>0 .ds $0 otroff
.	el .ie !'\*[$0]'unknown' \{\
.		ds $0 ditroff
.		if \n(.C=1 .do ds $0 groff
.	\}
.	el .if \n(.g=1 .ds $0 groff
.\}
.
.\" ASCII characters which are harder to typeset than you'd think
.ie '\*($0'heirloom' \{\
.	ds ^ \U'5E'
.	ds ~ \U'7E'
.	ds ' \U'27'
.	ds - \U'2D'
.	if !'\*(.T'html' .if t .ds - \(hy
.\}
.el .ie '\*($0'otroff' \{\
.	ds ^ ^
.	ds ~ ~
.	ds ' '
.	ds - \(hy
.\}
.el .ie n \{\
.	ds ^ \N'94'
.	ds ~ \N'126'
.	ds ' \N'39'
.	ds - \N'45'
.\}
.el \{\
.	ds ^ \N'0'
.	ds ~ \N'1'
.	ds ' \N'8'
.	ds - \N'45'
.\}
