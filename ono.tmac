.\" Hyperlink: Printed/non-interactive
.de [
.	if ']('\\$2' \{\
.		if !'\\$3'\\$1'         .if !'\\$3'\\$1/' \
.		if !'\\$3'http://\\$1'  .if !'\\$3'http://\\$1/' \
.		if !'\\$3'https://\\$1' .if !'\\$3'https://\\$1/' \
.		if !'\\$3'mailto:\\$1' \{\
.			ie ')'\\$6' \&\\$1\\$3\\$4\\$5\\$7
.			el \&\\$1 (\\*([_\\$3\\*(_])\\$5
.			ig  \}
\&\\*([_\\$1\\*(_]\\$5
.		 \}
.	if ']['\\$2' \{\
.		ie ']'\\$6' \&\\$1\\$3\\$4\\$5\\$7
.		el \&\\$1\\$5 \}
.	if ']'\\$2'  \&\\$1\\$3
..
.if n .ds [_ \fI
.if n .ds _] \fP
.
.\" Hyperlink: HTML
.nop .if r ps4html .rm [ \" HACK: Fix pre-grohtml(1)
.if '\*(.T'html' \{ .de [
.	if ']('\\$2' \{\
.		ie ')'\\$6' \X'html:<a href="\\$4">'\\$1\X'html:</a>'\\$7
.		el          \X'html:<a href="\\$3">'\\$1\X'html:</a>'\\$5 \}
.	if ']['\\$2' \{\
.		ie ']'\\$6' \X'html:<a href="#\\$4">'\\$1\X'html:</a>'\\$7
.		el          \X'html:<a href="#\\$3">'\\$1\X'html:</a>'\\$5 \}
.	if ']'\\$2' \X'html:<a href="#\\$1">'\\$1\X'html:</a>'\\$3
.. \}
.
.\" Hyperlink: PDF
.if '\*(.T'pdf' \{ .de [
.	if ']('\\$2' \{\
.		ie ')'\\$6' .nop .pdfhref W -A \&\\$7 -D "\\$4" -- \\$1
.		el          .nop .pdfhref W -A \&\\$5 -D "\\$3" -- \\$1 \}
.	if ']['\\$2' \{\
.		ie ']'\\$6' .nop .pdfhref L -A \&\\$7 -D "\\$4" -- \\$1
.		el          .nop .pdfhref L -A \&\\$5 -D "\\$3" -- \\$1 \}
.	if ']'\\$2' .nop .pdfhref L -A \&\\$3 -D "\\$1" -- \\$1
.. \}
.
.
.\" Set string $2 to ID associated with $1
.de #>
.	if \\n(.$>1 .rm \\$2
..
.
.\" Set register $2 to 1 if ID $1 exists, 0 otherwise
.de #?
.	nr \\$2 0
..
.
.\" Associate $1 with the ID "$2"
.de #<
.	am #> >>
.	if '\\\\$1'\\$1' .ds \\\\$2 \\$2
.	>>
.	am #? >>
.	if '\\\\$1'\\$2' .nr \\\\$2 1
.	>>
..
.
.\" Set string $1 to an unused identifier prefixed by $2
.de #+
.	nr +# \\n(+#+1
.	ds +# \\$2\\n(+#
.	#? \\*(+# #?
.	ie \\n(#?=1 .#+ \\$1 \\$2
.	el .ds \\$1 \\*(+#
.	rr #?
..
.nr +# 0
.
.
.\" Anchor: Handle preliminary logic shared across devices
.de #
.	ie \\n(.$=0 .ig #
.	el .ds $2 \\$2
.	#? "\\$1" #?
.
.	\" An ID was specified, and it isn't empty
.	if !'\\$2'' .if !'\\$2'\&' \{\
.		#< \\$1 \\$2
.		ig >> \}
.
.	\" No ID was given, or it was set to the empty string (\& or "")
.	ie \\n(#?=1 .#> "\\$1" $2  \" Reuse an existing ID if one exists
.	el          .#+ $2 id- \}  \" Otherwise, generate a new one
.
.	ig >>
.	>>
..
.
.nr ## 0 1 \" Counter used when generating IDs automatically
.nr #L 1   \" Outline level (PDF only)
.
.\" Anchor: Printed/non-interactive
.if !'\*(.T'html' \{\
.if !'\*(.T'pdf'  \{\
.am #
.	if !'\\$1'' \&\\$1\\$3
.. \}\}
.
.\" Anchor: HTML
.nop .if r ps4html .rm #
.if '\*(.T'html' \{ .am #
.	if '\\$1'' .>>
.	ie '\\$1\\$3'' \X'html:<a name="\\*($2"></a>'\c
.	el             \X'html:<a name="\\*($2">'\\$1\X'html:</a>'\\$3
.. \}
.
.\" Anchor: PDF
.if '\*(.T'pdf' \{\
.nop .ds PDFBOOKMARK.VIEW /XYZ 0 \\n[PDFPAGE.Y] u 0
.nop .ds PDFHREF.VIEW     /XYZ 0 \\n[PDFPAGE.Y] u 0
.am #
.
.	\" Unless $1 is empty, include an outline entry
.	if !'\\$1'' .if !'\\$1'\&' \{\
.		nop .pdfbookmark \\n(#L "\\$1"
.		ig >> \}
.
.	\" Otherwise, insert only a named destination
.	nop .pdfmark M -N "\\*($2"
.
.	ig >>
.	>>
.. \}
.
.\" Anchor: Cleanup
.am #
.	rm $2
.	rr #?
.	#
..
